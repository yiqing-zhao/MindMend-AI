/**index.wxss**/
.container {
  width: 100%;
  height: 100%;
  background-color: #f8f4f1;
}

.bg {
  width: 100%;
  max-width: 100%;
  position: absolute;
}

.layer {
  width: 74%;
  height: 60%;
  position: absolute;
  top: -10px;
  left: 26%;
}

.sub-title {
  width: 100%;
  box-sizing: border-box;
  padding: 116px 5% 0px 5%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
  opacity: 60%;
  font-size: xx-large;
  color: white;
}

.main-title {
  width: 100%;
  box-sizing: border-box;
  padding-top: 0;
  padding: 16px 5% 0px 5%;
  display: flex;
  align-items: center;
  position: relative;
  font-size: xx-large;
  font-weight: bold;
  color: white;
}

.custom-navbar {
  --td-navbar-color: #ffffff;
  --td-navbar-bg-color: #0000000;
}

.custom-title {
  font-size: x-large;
  font-weight: bold;
}

.diagnostic {
  position: absolute;
  width: 100%;
  top: 66.5%;
  text-align: center;
  box-sizing: content-box;
  filter: drop-shadow(0px 6px 10px #000000 10%);
}

.chatbot {
  position: absolute;
  width: 100%;
  top: 84.7%;
  text-align: center;
  box-sizing: content-box;
}

.button {
  width: 60%;
  height: 60px;
  font-size: 14px;
  font-weight: bold;
  color: #ffffff;
  background-color: #4b3426;
  border: transparent;
  border-radius: 10px;
  text-align: center;
  padding-top: 10px;
}
